{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:line",
          "template": "\u250c\u2500"
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "p:black",
          "background": "p:yellow",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": " {{ if .SSHSession }}\ueba9 {{ .UserName }} {{ end }} "
        },
        {
          "type": "path",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "foreground": "p:black",
          "background": "p:blue",
          "properties": {},
          "template": "{{ $segment := .Segments.Git }}{{ $icon := \"\uf1d3\" }}{{ if $segment.Dir }} {{ $icon }} <i><b>{{ $segment.RepoName }}{{ if $segment.IsWorkTree}} \ue21c{{ end }}</b></i>{{if ne $segment.RepoName .Path}} \ueaf7 {{ trimPrefix $segment.Dir .PWD }}{{ end }}{{ else }} \uea83 {{ path .Path .Location }}{{ end }}{{ if not .Writable }} \uf023{{ end }} "
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "foreground": "p:black",
          "background": "p:green",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:orange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:yellow{{ end }}",
            "{{ if gt .Behind 0 }}p:yellow{{ end }}"
          ],
          "template": " {{ if .UpstreamURL }}{{ url .UpstreamIcon .UpstreamURL }} {{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "properties": {
            "branch_max_length": 25,
            "fetch_status": true,
            "fetch_upstream_icon": false,
            "branch_icon": " "
          }
        },
        {
          "type": "project",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "foreground": "p:black",
          "background": "p:teal",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Version }} {{.Version}}{{ end }}{{ end }} "
        },
        {
          "type": "python",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "foreground": "p:black",
          "background": "p:magenta",
          "template": "  {{ .Venv }} {{ .Full }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:line",
          "template": "\u2514"
        },
        {
          "type": "status",
          "style": "diamond",
          "foreground": "p:black",
          "background": "p:red",
          "leading_diamond": "\ue0ba",
          "trailing_diamond": "\ue0bc",
          "template": "{{ if .Error }}\u00d7{{ .Code }}{{ end }}"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "p:yellow",
          "template": "$([ $(jobs | wc -l | xargs) -gt 0 ] && echo '<p:line,transparent>\u2500</>\uf0ad ')"
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "p:green",
          "background": "transparent",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ],
          "template": "<p:line,transparent>\u2500</>\u276f",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ],
  "transient_prompt": {
    "foreground": "p:line",
    "background": "transparent",
    "template": "\u276f "
  },
  "secondary_prompt": {
    "foreground": "p:black",
    "background": "transparent",
    "template": "<p:yellow,transparent>\ue0ba</><,p:yellow> > </><p:yellow,transparent>\ue0b0</> "
  },
  "palette": {
    "line": "#5b6078",
    "black": "#1b1d2b",
    "red": "#ff757f",
    "green": "#c3e88d",
    "yellow": "#ffc777",
    "blue": "#82aaff",
    "magenta": "#c099ff",
    "cyan": "#86e1fc",
    "white": "#828bb8",
    "background": "#222436",
    "foreground": "#c8d3f5",
    "orange": "#ff966c",
    "teal": "#4fd6be"
  }
}
